{
  "name": "wingspan-score-app",
  "version": "1.2.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "icons:process": "bash scripts/generate_macos_icon.sh base_icon.png",
    "icons:expo": "cp final_icon.png assets/icon.png && cp final_icon.png assets/adaptive-icon.png && convert final_icon.png -resize 512x512 assets/icon-512.png && echo 'âœ“ Copied icons to assets/ (including 512x512)'",
    "icons:ios": "bash scripts/generate-ios-icons.sh",
    "icons:android": "bash scripts/generate-android-icons.sh",
    "icons:all": "npm run icons:process && npm run icons:expo && npm run icons:ios && npm run icons:android",
    "icons:fix-ios": "bash scripts/fix-ios-icons.sh",
    "images:optimize": "bash scripts/optimize-images.sh",
    "version": "node scripts/sync-version.js && git add app.json",
    "check:contrast": "npx ts-node scripts/checkColorContrast.ts"
  },
  "dependencies": {
    "@expo-google-fonts/dm-mono": "^0.2.3",
    "@expo-google-fonts/playfair-display": "^0.2.3",
    "@expo-google-fonts/source-sans-3": "^0.2.3",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "expo": "~54.0.30",
    "expo-crypto": "~15.0.8",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-sqlite": "~16.0.3",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "0.5.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2"
